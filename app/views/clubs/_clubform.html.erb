<%= form_for(@club) do |f| %>
  <% if @club.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@club.errors.count, "error") %> prohibited this club from being saved:</h2>

      <ul>
      <% @club.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

 <div class="field">
    <%= f.label :name %><br>
    <%= f.select :club, [['Cap and Gown','cap'],['Cannon','cannon'],['Charter ','charter'],['Cloister','cloister'],['Colonial','cap and gown'],['Cottage ','cottage'],['Ivy ','ivy'],['Quad ','quad'],['Terrace','terrace'],['Tower ','tower']]%> 
  </div>
  <div class="field">
    <%= f.label :email %> (Princeton)<br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div class="field">
      <%= f.label :recipient_id %> 
      <% f.text_field :recipient_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
